<section>
    <div class="container">
      <a href="/equipments" class="btn btn-success mt-5" style="margin-right: 50px; "> Back</a>
      <div class="row mt-1 mb-1">
       
         <button class="btn btn-success" style="margin-left: 900px ;" onclick="window.print()">Print this page</button>
       
      </div>
      <h1 style="font-family: 'Times New Roman', Times, serif;" class="text-center  text-light">EECO</h1>
      <table class="table mt-5" id="table_id">
  <thead>
    <tr>
 
   <th scope="col">No:</th>
         
          <th scope="col">User Name</th>
          <th scope="col">Date</th>
          
          
     
    </tr>
  </thead>
  <tbody class="text-dark">
      {{#each EECODetails}}
    <tr>

        <td>{{this.no}}</td>
          <td>{{this.progress-method}}</td>
          <td>{{this.date}}</td>
      

     
    
    </tr>
  {{/each}}
  </tbody>
</table> 
    </div>

</section>


<script>
$(document).ready( function () {
    $('#table_id').DataTable();
} );
</script>